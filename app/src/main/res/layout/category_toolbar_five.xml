<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="700dp"
    android:background="@color/colorFive"
    android:orientation="vertical">


    <include layout="@layout/category_toolbar_four"
        android:id="@+id/include4" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/iconTravel"
        android:src="@drawable/travel"
        android:layout_marginRight="16dp"
        android:layout_marginEnd="16dp"
        android:layout_alignBottom="@+id/txtTravel"
        android:layout_toLeftOf="@+id/txtTravel"
        android:layout_toStartOf="@+id/txtTravel" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Travel"
        android:id="@+id/txtTravel"
        android:padding="10dp"
        android:textColor="@color/colorPrimaryText"
        android:layout_below="@+id/include4"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp" />




    </RelativeLayout>

